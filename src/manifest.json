{
  "name": "sanaz",
  "description": "Sanaz is Asssiter extention",
  "version": null,
  "manifest_version": 2,
  "icons": {
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "web_accessible_resources": [
    "main.js"
  ],
  "browser_action": {
    "default_title": "sanaz"
  },
  "content_scripts": [
    {
      "matches": [
        "https://*/*",
        "http://*/*"
      ],
      "js": [
        "app/main.js"
      ],
      "css": [
        "app/main.css"
      ]
    }
  ],
  "permissions": [
    "storage",
    "activeTab",
    "https://*/*",
    "http://*/*",
    "tabs",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "background.js",
      "main.js"
    ]
  }
}
